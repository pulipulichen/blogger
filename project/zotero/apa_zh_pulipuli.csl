  <!---->

  <macro name="locators">
    <choose>
      <if type="article-journal article-magazine" match="any">
        <group prefix=", " delimiter=", ">
          <group>
            <group>
              <text variable="volume"/>
            </group>
            <text variable="issue" prefix="(" suffix=")"/>
          </group>
          <text variable="page"/>
        </group>
      </if>
      <else-if type="article-newspaper">
        <group delimiter=" " prefix=", ">
          <label variable="page" form="short"/>
          <text variable="page"/>
        </group>
      </else-if>
      <else-if type="book graphic motion_picture report song chapter paper-conference" match="any">
        <group prefix=" (" suffix=")" delimiter=", ">
          <text macro="edition"/>
          <group>
            <text term="volume" form="short" plural="true" text-case="capitalize-first" suffix=" "/>
            <number variable="number-of-volumes" form="numeric" prefix="1-"/>
          </group>
          <group>
            <text term="volume" form="short" text-case="capitalize-first" suffix=" "/>
            <number variable="volume" form="numeric"/>
          </group>
          <group>
            <label variable="page" form="short" suffix=" "/>
            <text variable="page"/>
          </group>
        </group>
      </else-if>
      <else-if type="legal_case">
        <group prefix=" (" suffix=")" delimiter=" ">
          <text variable="authority"/>
          <date variable="issued" form="text"/>
        </group>
      </else-if>
      <else-if type="bill legislation" match="any">
        <date variable="issued" prefix=" (" suffix=")">
          <date-part name="year"/>
        </date>
      </else-if>
    </choose>
  </macro>

  <macro name="locators-zh">
    <choose>
      <if type="article-journal article-magazine" match="any">
        <group delimiter="，" suffix="">
          <group>
            <choose>
              <if variable="volume">
                <group font-style="italic">
                  <text variable="volume" />
                </group>
                <text variable="issue" prefix="（" suffix="）"/>
              </if>
              <else>
                <group font-style="italic">
                  <text variable="issue"/>
                </group>
              </else>
            </choose>
          </group>
          <text variable="page" />
         </group>
      </if>
      <if type="article-newspaper" match="any">
        <group delimiter="，" suffix="">
          <group>
            <group font-style="italic">
              <text variable="volume" />
            </group>
            <text variable="issue" prefix="（" suffix="）"/>
          </group>
          <text variable="page" prefix="第 " suffix=" 版" />
         </group>
      </if>
      <else-if type="legal_case">
        <group prefix="（" suffix="）" delimiter=" ">
          <text variable="authority"/>
          <date variable="issued" form="text"/>
        </group>
      </else-if>
      <else-if type="bill legislation" match="any">
        <date variable="issued" prefix="（" suffix="）">
          <date-part name="year"/>
        </date>
      </else-if>
      
    </choose>


  </macro>

  <!---->

  <macro name="citation-locator">
    <group>
      <choose>
        <if locator="chapter">
          <label variable="locator" form="long" text-case="capitalize-first"/>
        </if>
        <else>
          <label variable="locator" form="short"/>
        </else>
      </choose>
      <text variable="locator" prefix=" "/>
    </group>
  </macro>

  <macro name="citation-locator-zh">
    <group>
      <!--
      <label variable="locator" include-period="true" form="short"/>
      <text variable="locator" prefix=" "/>
      -->
      <text variable="locator" prefix="頁 "/>
    </group>
  </macro>




  <!---->

  <macro name="container">
    <group delimiter=" ">
      <choose>
        <if type="chapter paper-conference entry-encyclopedia" match="any">
          <text term="in" text-case="capitalize-first" suffix=" "/>
        </if>
      </choose>
      <text macro="container-contributors"/>
      <text macro="secondary-contributors"/>
      <text macro="container-title" font-style="italic"/>
    </group>
  </macro>

  <!---->

  <macro name="container-title">
    <choose>
      <if type="article article-journal article-magazine article-newspaper" match="any">
        <group font-style="italic">
          <text variable="container-title" text-case="title"/>
        </group>
      </if>
      <else-if type="bill legal_case legislation" match="none">
        <group font-style="italic">
          <text variable="container-title"/>
        </group>
      </else-if>
      <else>
        <group delimiter=" " prefix=", ">
          <choose>
            <if variable="container-title">
              <text variable="volume"/>
              <group font-style="italic">
                <text variable="container-title"/>
              </group>
              <group delimiter=" ">
                <!--change to label variable="section" as that becomes available -->
                <text term="section" form="symbol"/>
                <text variable="section"/>
              </group>
              <text variable="page"/>
            </if>
            <else>
              <choose>
                <if type="legal_case">
                  <text variable="number" prefix="No. "/>
                </if>
                <else>
                  <text variable="number" prefix="Pub. L. No. "/>
                  <group delimiter=" ">
                    <!--change to label variable="section" as that becomes available -->
                    <text term="section" form="symbol"/>
                    <text variable="section"/>
                  </group>
                </else>
              </choose>
            </else>
          </choose>
        </group>
      </else>
    </choose>
  </macro>

  <!---->

  <macro name="chinese-bibliography">

    <!-- <text macro="chinese-sort" /> -->

    <text macro="author-zh" suffix=""/>
    <text macro="issued-zh" suffix=""/>
    <group delimiter="。">
      <text macro="title-zh"/>
      <group delimiter="，">
        <choose>
          <if type="book chapter" match="none">
            <text macro="chinese-container-title"/>
            <group>
              <text variable="collection-title"/>
            </group>
          </if>
          <else-if type="paper-conference" match="any">
            <text macro="chinese-container-title"/>
          </else-if>
        </choose>
        <text macro="locators-zh"/>
      </group>
      <text macro="publisher-zh"/>
    </group>
    <text macro="access-zh"/>
  </macro>

  <macro name="chinese-container-title">
    <group font-style="italic">
      <text variable="container-title"  />
    </group>
    <choose>
      <if type="paper-conference" match="any">
        <text variable="page" prefix="（頁" suffix="）"/>
      </if>
    </choose>

  </macro>

  <!---->

  <macro name="chinese-sort">
    <choose>
      <if locale="en-us en English english Anglais" match="any">
        <text value="2"/>
      </if>
      <else-if locale="zh-tw zh-cn Chinese chi chinese zh-Hant zh_TW jp 中文 繁體中文 簡體中文" match="any">
        <text value="1"/>
      </else-if>
      <else-if variable="language" match="none">
        <text value="2"/>
      </else-if>
      <else>
        <text value="2"/>
      </else>
    </choose>

    <!-- <text value=" | "/> -->
  </macro>

  <macro name="author-sort">
    <choose>
      <if locale="en-us en English english Anglais" match="any">
        <text marco="author"/>
      </if>
      <else-if locale="zh-tw zh-cn Chinese chi chinese zh-Hant zh_TW 中文 繁體中文 簡體中文" match="any">
        <text marco="author-zh"/>
      </else-if>
      <else-if variable="language" match="none">
        <text marco="author"/>
      </else-if>
      <else>
        <text marco="author"/>
      </else>
    </choose>

    <!-- <text value=" | "/> -->
  </macro>

  <!---->

  <citation et-al-min="6"
            et-al-use-first="1"
            et-al-subsequent-min="3"
            et-al-subsequent-use-first="1"
            disambiguate-add-year-suffix="true" disambiguate-add-names="true"
            disambiguate-add-givenname="true" collapse="year"
            givenname-disambiguation-rule="primary-name">

    <sort>
      <key macro="chinese-sort" sort="ascending"/>
      <key macro="author"/>
      <key macro="issued-sort"/>
    </sort>

    <layout locale="zh-tw" prefix="（" suffix="）" delimiter="；">
        <group delimiter="，">
          <text macro="author-short-zh"/>
          <text macro="issued-year-zh"/>
          <text macro="citation-locator-zh"/>
        </group>
    </layout>
    <layout locale="zh-cn" prefix="（" suffix="）" delimiter="；">
        <group delimiter="，">
          <text macro="author-short-zh"/>
          <text macro="issued-year-zh"/>
          <text macro="citation-locator-zh"/>
        </group>
    </layout>
    <layout locale="Chinese" prefix="（" suffix="）" delimiter="；">
        <group delimiter="，">
          <text macro="author-short-zh"/>
          <text macro="issued-year-zh"/>
          <text macro="citation-locator-zh"/>
        </group>
    </layout>
    <layout locale="zh-Hant" prefix="（" suffix="）" delimiter="；">
        <group delimiter="，">
          <text macro="author-short-zh"/>
          <text macro="issued-year-zh"/>
          <text macro="citation-locator-zh"/>
        </group>
    </layout>
    <layout locale="chi" prefix="（" suffix="）" delimiter="；">
        <group delimiter="，">
          <text macro="author-short-zh"/>
          <text macro="issued-year-zh"/>
          <text macro="citation-locator-zh"/>
        </group>
    </layout>
	<layout locale="jp" prefix="（" suffix="）" delimiter="；">
        <group delimiter="，">
          <text macro="author-short-zh"/>
          <text macro="issued-year-zh"/>
          <text macro="citation-locator-zh"/>
        </group>
    </layout>

    <!---->

    <layout prefix="(" suffix=")" delimiter="; ">
      <group delimiter=", ">
        <text macro="author-short"/>
        <text macro="issued-year"/>
        <text macro="citation-locator"/>
      </group>
    </layout>
  </citation>

  <!---->

  <bibliography hanging-indent="true"
                et-al-min="8"
                et-al-use-first="6"
                et-al-use-last="true"
                entry-spacing="0"
                line-spacing="1.5">
    <sort>
      <key macro="chinese-sort" sort="ascending"/>
    <key macro="author" sort="ascending"/>
      <!-- <key macro="author-sort" sort="ascending"/> -->
      <!-- <key variable="language" sort="descending"/> -->

      <key macro="issued-sort" sort="ascending"/>
      <key macro="title"/>
    </sort>

    <!-- 中文的格式 -->
    <layout locale="zh-tw">
      <text macro="chinese-bibliography"/>
    </layout>
    <layout locale="zh-cn">
      <text macro="chinese-bibliography"/>
    </layout>
    <layout locale="zh-Hant">
      <text macro="chinese-bibliography"/>
    </layout>
    <layout locale="Chinese">
      <text macro="chinese-bibliography"/>
    </layout>
    <layout locale="chi">
      <text macro="chinese-bibliography"/>
    </layout>
	<layout locale="jp">
      <text macro="chinese-bibliography"/>
    </layout>

    <layout>
      <!-- <text macro="chinese-sort" /> -->
      <group suffix=".">
        <group delimiter=". ">
